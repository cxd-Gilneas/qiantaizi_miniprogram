.data-entry-section {
  display: flex;
  width: 100%;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.data-entry-section .data-entry-title {
  flex: 3;
  justify-content: center;
  align-content: center;
  font-size: 35rpx;
}
.data-entry-section .data-entry {
  flex: 2;
  font-size: 36rpx;
}
.data-entry-section .data-entry button {
  width: 100%;
}
.btn .button {
  width: 100%;
}
.btn .button button {
  margin-top: 100rpx;
  margin-bottom: 100rpx;
}
